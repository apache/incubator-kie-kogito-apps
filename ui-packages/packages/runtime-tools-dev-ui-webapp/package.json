    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-operations": "^2.1.4",
    "@graphql-codegen/typescript-react-apollo": "^3.1.4",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack": "^4.39.1",
    "apollo-server-express": "^2.14.2",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "core-js": "3.6.5",
    "css-loader": "^5.2.6",
    "cypress": "^9.6.1",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^6.1.4",
    "html-webpack-plugin": "^5.3.2",
    "https-browserify": "^1.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.2",
    "openapi-types": "^9.3.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.3.0",
    "stream-http": "^3.2.0",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^4.0.0",
    "swagger-ui-express": "4.3.0",
    "ts-loader": "^9.4.2",
    "url-loader": "^4.1.1",
    "uuid": "^3.3.3",
    "waait": "^1.0.5",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.8.0",
    "webpack-merge": "^5.8.0",
    "typescript": "^4.4.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "ts-jest": "^26.5.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^10.0.0",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.23",
    "@testing-library/jest-dom": "^5.12.0",
    "@types/enzyme": "^3.10.8",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "babel-jest": "^25.1.0"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "setupFiles": [
      "./config/Jest-config/test-shim.js",
      "./config/Jest-config/test-setup.js",
      "core-js"
    ],
    "coveragePathIgnorePatterns": [
      "./src/static",
      "./tests/mocks/"
    ],
    "coverageReporters": [
      [
        "lcov",
        {
          "projectRoot": "../../"
        }
      ]
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [],
    "transform": {
      "^.+\\.jsx?$": "./config/Jest-config/babel-jest-wrapper.js",
      "^.+\\.(ts|tsx)$": "ts-jest",
      "\\.(jpg|jpeg|png|svg)$": "./config/Jest-config/fileMocks.js"
    },
    "testMatch": [
      "**/tests/*.(ts|tsx)"
    ],
    "moduleNameMapper": {
      "\\.(scss|sass|css)$": "identity-obj-proxy",
      "monaco-editor": "<rootDir>/__mocks__/monacoMock.js"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "pnpm run format",
      "pnpm run lint"
    ]
  }
}
